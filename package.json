    "madge": "madge projects/sage-angular/src/public-api.ts --ts-config tsconfig.json --image graph.png",
    "storybook": "ng run sage-angular-demo:storybook",
    "build:storybook": "ng run sage-angular-demo:build-storybook",
    "release": "release-it"
  },
