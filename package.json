{
  "name": "@sage-bionetworks/sage-angular",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:demo": "ng build sage-angular-demo",
    "test": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "analyze": "ng build --prod --project sage-angular-demo --stats-json && webpack-bundle-analyzer ./dist/sage-angular-demo/stats.json",
    "madge": "madge projects/sage-angular/src/public-api.ts --ts-config tsconfig.json --image graph.png",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006 --ci --static-dir ./node_modules/@angular/material/prebuilt-themes",
    "build-storybook": "npm run docs:json && build-storybook --static-dir ./node_modules/@angular/material/prebuilt-themes"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.3",
    "@angular/cdk": "^11.2.9",
    "@angular/common": "~11.2.3",
    "@angular/compiler": "~11.2.3",
    "@angular/core": "~11.2.3",
    "@angular/forms": "~11.2.3",
    "@angular/material": "^11.2.9",
    "@angular/platform-browser": "~11.2.3",
    "@angular/platform-browser-dynamic": "~11.2.3",
    "@angular/router": "~11.2.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.3",
    "@angular/cli": "~11.2.2",
    "@angular/compiler-cli": "~11.2.3",
    "@babel/core": "^7.13.15",
    "@compodoc/compodoc": "^1.1.11",
    "@storybook/addon-actions": "^6.2.8",
    "@storybook/addon-essentials": "^6.2.8",
    "@storybook/addon-knobs": "^6.2.8",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/angular": "^6.2.8",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^14.14.35",
    "babel-loader": "^8.2.2",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "madge": "^4.0.2",
    "ng-packagr": "^11.0.0",
    "ng-samurai": "^2.0.6",
    "protractor": "~7.0.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5",
    "webpack-bundle-analyzer": "^4.4.1"
  }
}
